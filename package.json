{
    "name": "hunger-station-system-page-keeper",
    "version": "1.0.0",
    "description": "Service to maintain HungerStation system page session",
    "main": "dist/server.js",
    "scripts": {
      "start": "node dist/server.js",
      "dev": "ts-node-dev --respawn --transpile-only server.ts",
      "build": "tsc",
      "lint": "eslint **/*.ts",
      "lint:fix": "eslint **/*.ts --fix",
      "format": "prettier --write \"**/*.ts\"",
      "clean": "rimraf dist",
      "prebuild": "npm run clean",
      "test": "jest",
      "prepare": "husky install"
    },
    "dependencies": {
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "puppeteer": "^21.5.0",
      "winston": "^3.11.0"
    },
    "devDependencies": {
      "@types/express": "^4.17.21",
      "@types/jest": "^29.5.8",
      "@types/node": "^20.9.0",
      "@typescript-eslint/eslint-plugin": "^6.11.0",
      "@typescript-eslint/parser": "^6.11.0",
      "eslint": "^8.53.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-prettier": "^5.0.1",
      "husky": "^8.0.3",
      "jest": "^29.7.0",
      "prettier": "^3.1.0",
      "rimraf": "^5.0.5",
      "ts-jest": "^29.1.1",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.2.2"
    },
    "engines": {
      "node": ">=16.0.0"
    },
    "author": "",
    "license": "ISC"
  }